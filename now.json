{
  "name": "engine262-api",
  "version": 2,
  "routes": [
    {
      "src": "/api/gist",
      "dest": "/api/gist.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "*"
      }
    },
    {
      "src": "/download",
      "dest": "/api/download.js"
    }
  ],
  "env": {
    "GH_TOKEN": "@gh_token"
  }
}
