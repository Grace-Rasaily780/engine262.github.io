{
  "name": "engine262-api",
  "version": 2,
  "routes": [
    {
      "src": "/api/gist",
      "dest": "/api/gist.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "*"
      }
    },
    {
      "src": "/engine262/engine262.js",
      "dest": "/api/bridge.js"
    },
    {
      "src": "/engine262/engine262.js.map",
      "dest": "/api/bridge.js"
    }
  ],
  "env": {
    "GH_TOKEN": "@gh_token"
  }
}
