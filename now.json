{
  "name": "engine262-api",
  "version": 2,
  "routes": [
    {
      "src": "/api/gist",
      "dest": "/api/gist.js",
      "headers": {
        "Access-Control-Allow-Headers": "content-type"
      }
    },
    {
      "src": ".*",
      "status": 302,
      "headers": { "Location": "https://engine262.js.org" }
    }
  ],
  "env": {
    "GH_TOKEN": "@gh_token"
  }
}
